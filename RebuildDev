node {
    parameters {
        string(name: 'gitUrl', defaultValue: 'https://github.com/iwon01/testApps.git', description: 'Git url of project')
    }
    stage('Clean workspace') {
        cleanWs()
    }
    stage('Checkout from SCM') {
        echo 'Checking out from SCM'
        checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: 'git', url: "${params.gitUrl}"]]])

    }
    stage('Delete dev branch remote') {
        try {
            echo 'Deleting remote dev branch...'
            bat 'git push -d origin develop'
        } catch (ex) {
            echo ex.toString()
        }
    }
    stage('Rebuild dev branch') {
        echo 'Rebuilding dev branch...'
        bat 'git checkout staging'
        bat 'git checkout -b develop'
        bat 'git push --set-upstream origin develop'
    }
    stage('Clean workspace') {
        cleanWs()
    }
}